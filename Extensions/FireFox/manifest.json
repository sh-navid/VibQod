{
  "manifest_version": 2,
  "name": "VibQod",
  "version": "1.1",
  "description": "Loads localhost:9090 in the popup.",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "Logos/logo16.png",
      "32": "Logos/logo32.png",
      "48": "Logos/logo48.png",
      "128": "Logos/logo128.png"
    }
  }
}
